<%- include('includes/head.ejs') %>
<link rel="stylesheet" href="/css/users.css">
</head>

<body>
<%- include('includes/navigation.ejs') %>
<main>
    <% if (usersList.length > 0) { %>
        <% for (let name of usersList) { %>
            <ul class="users-list">
                <li class="users_list__item">
                    <%= name %>
                </li>
            </ul>
        <% } %>
    <% } else { %>
        <h1>No users yet...</h1>
    <% } %>
</main>
<%- include('includes/end.ejs') %>